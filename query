PREFIX crm: <http://www.cidoc-crm.org/cidoc-crm/>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX crmdig: <http://www.ics.forth.gr/isl/CRMdig/>
PREFIX custom: <https://pharos.artresearch.net/custom/>
CONSTRUCT {  ?s custom:has_image ?image }
WHERE
{
	?s crm:P138i_is_represented_by ?visual_item .
	?visual_item crm:P128i_is_carried_by ?photo .
	?photo crm:P129i_is_subject_of ?image .
	?image crm:P2_has_type <http://iiif.io/api/image> .
	?image crm:P2_has_type ?recto .
	?recto rdfs:label "recto" .
}